---
import HeroHeader from "../../components/HeroHeader.astro";
import { HeroParallax } from "../../components/tsx/hero-parallax";
import type { Project } from "../../shared/payload-types";

// props
interface ProjectHeroProps {
  projects: Project[];
}

const { projects } = Astro.props as ProjectHeroProps;
---

<HeroParallax products={projects} client:load>
  <HeroHeader />
</HeroParallax>
